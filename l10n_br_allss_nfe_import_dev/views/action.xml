<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_l10n_br_allss_wizard_nfe_import" model="ir.actions.act_window">
        <field name="name">Importação de XML da NFe</field>
        <field name="res_model">l10n.br.allss.wizard.nfe.import</field>
        <field name="view_id" ref="view_l10n_br_allss_wizard_nfe_import_form"/>
        <field name="view_mode">form</field>
        <field name="target">inline</field>
        <field name="context">{'allss_nfe_import': True}</field>
    </record>

    <record id="action_l10n_br_allss_nfe_mde" model="ir.actions.act_window">
        <field name="name">ALLSS Manifesto do Destinatário</field>
        <field name="res_model">l10n.br.allss.nfe.mde</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_l10n_br_allss_nfe_mde_tree"/>
        <field name="search_view_id" ref="view_l10n_br_allss_nfe_mde_search"/>
        <field name="context">{"search_default_situacao_pendente":1}</field>
    </record>

    <record id="action_l10n_br_allss_wizard_nfe_schedule" model="ir.actions.act_window">
        <field name="name">Verificar NF-e Destinada</field>
        <field name="res_model">l10n.br.allss.wizard.nfe.schedule</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_l10n_br_allss_wizard_nfe_schedule_form"/>
        <field name="target">new</field>
    </record>
</odoo>
