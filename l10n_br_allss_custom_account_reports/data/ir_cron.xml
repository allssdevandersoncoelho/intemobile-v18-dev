    </record>

    <record id="balance_structure_general_calc" model="ir.cron">
        <field name="name">Balancete Estruturado - Cálculo Geral Balanço</field>
        <field name="model_id" ref="account.model_account_move_line"/>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="priority">5</field>
        <field name="code">
            model.calculation_balances_general()
            <!-- #model.calculation_balances_general_tag() -->
            model.calculation_balances_general_struc()
        </field>
    </record>

