<odoo>
    <record id="balance_structure_update_data" model="ir.cron">
        <field name="name">Balancete Estruturado - Atualiza Dados</field>
        <field name="model_id" ref="l10n_br_allss_custom_account_reports.model_allss_balance_account_structure"/>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="False"/>
        <field name="priority">1</field>
        <field name="code">
            model.execute_sql()
            model.init_account_structure()
        </field>
    </record>

