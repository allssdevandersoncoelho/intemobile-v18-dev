<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_l10n_br_allss_nfe_import_teste_inherit_res_company_form" model="ir.ui.view">
        <field name="name">ALLSS NFe Import Company Data</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="after">
                <newline/>
                <group string="NF-e Automation" groups="base.group_no_one,base.group_system">
                    <field name="l10n_br_allss_last_nsu_nfe" groups="base.group_system"/>
                    <field name="l10n_br_allss_manifest_automation"/>
                    <field name="l10n_br_allss_fiscal_position_id_automation"/>
                    <field name="l10n_br_allss_partner_automation"/>
                    <field name="l10n_br_allss_invoice_automation"/>
                    <field name="l10n_br_allss_tax_automation"/>
                    <field name="l10n_br_allss_supplierinfo_automation"/>
                    <field name="l10n_br_allss_purchase_order_automation"/>
                    <field name="l10n_br_allss_vendor_journal_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
